{
  "manifest_version": 3,
  "name": "Demo Extension",
  "version": "1.0",
  "description": "This is a demo Chrome extension.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  }
}
